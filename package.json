{
    "name": "@dhis2/analytics",
    "version": "2.4.16",
    "main": "./build/index.js",
    "repository": "git@github.com:dhis2/analytics.git",
    "author": "Jennifer Jones Arnesen <jennifer@dhis2.org>",
    "license": "BSD-3-Clause",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prebuild": "rm -rf ./build/*",
        "build": "npm run localize && BABEL_ENV=commonjs babel src --out-dir ./build --copy-files --verbose",
        "postbuild": "yarn build-storybook",
        "build-storybook": "build-storybook",
        "lint": "d2-style js check || d2-style text check",
        "start-storybook": "start-storybook --port 5000",
        "start": "yarn start-storybook",
        "localize": "npm run extract-pot && d2-i18n-generate -l -n default -p ./i18n/ -o ./src/locales/",
        "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
        "test": "jest"
    },
    "husky": {
        "hooks": {
            "commit-msg": "d2-style commit check",
            "pre-commit": "d2-style js apply"
        }
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/plugin-transform-react-constant-elements": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.4.3",
        "@babel/plugin-transform-shorthand-properties": "^7.2.0",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@dhis2/cli-style": "^7.0.0",
        "@dhis2/d2-i18n": "^1.0.6",
        "@dhis2/d2-i18n-extract": "^1.0.8",
        "@dhis2/d2-i18n-generate": "1.2.0",
        "@sambego/storybook-state": "^1.3.4",
        "@storybook/addon-actions": "^5.0.6",
        "@storybook/addon-links": "^5.0.6",
        "@storybook/addons": "^5.0.6",
        "@storybook/react": "^5.0.6",
        "babel-loader": "^8.0.5",
        "babel-polyfill": "^6.26.0",
        "conventional-changelog-cli": "^2.0.5",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.2",
        "fs-extra": "^7.0.0",
        "husky": "^1.0.1",
        "jest": "24.7.1",
        "jest-enzyme": "^7.0.2",
        "react-dev-utils": "^8.0.0",
        "typeface-roboto": "^0.0.54"
    },
    "peerDependencies": {
        "@dhis2/d2-i18n": "^1.0.6",
        "prop-types": "^15",
        "react": "^16.3",
        "react-dom": "^16.3"
    },
    "dependencies": {
        "@dhis2/d2-ui-org-unit-dialog": "^7.0.4",
        "@dhis2/ui-core": "^3.4.0",
        "@material-ui/core": "^3.9.3",
        "@material-ui/icons": "^3.0.2",
        "classnames": "^2.2.6",
        "d2-utilizr": "^0.2.16",
        "d3-color": "^1.2.3",
        "highcharts": "^7.1.2",
        "lodash": "^4.17.13",
        "react-beautiful-dnd": "^10.1.1",
        "styled-jsx": "^3.2.1"
    },
    "sideEffects": [
        "./src/locales/index.js"
    ],
    "files": [
        "build"
    ]
}
